<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HalfBackgroundTextView">
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="round_type" format="enum">
            <enum name="top_left" value="0"/>
            <enum name="top_right" value="1"/>
            <enum name="bottom_right" value="2"/>
            <enum name="bottom_left" value="3"/>
            <enum name="top" value="4"/>
            <enum name="right" value="5"/>
            <enum name="bottom" value="6"/>
            <enum name="left" value="7"/>
            <enum name="all" value="8"/>
        </attr>
        <attr name="round_radius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="Banner">
        <attr name="mode" format="enum">
            <enum name="normal" value="0"/>
            <enum name="clip" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ConstraintShadowLayout">
        <attr name="cornerRadiusC" format="dimension" />
        <attr name="shadowRadiusC" format="dimension" />
        <attr name="shadowColorC" format="color" />
        <attr name="fillColorC" format="color" />
        <attr name="dxC" format="dimension" />
        <attr name="dyC" format="dimension" />
        <attr name="asideValidC" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="CustomCardView">
        <attr name="cardCornerRadius" format="dimension"/>
        <attr name="cardShadowColor" format="color"/>
        <attr name="cardBackgroundColor" format="color"/>
        <attr name="cardDy" format="dimension"/>
        <attr name="cardShadowSize" format="dimension"/>
    </declare-styleable>
</resources>